<Application x:Class="FlowCommander.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\Data.xaml" />
                <ResourceDictionary Source="Resources\CommonStyles.xaml" />
                <ResourceDictionary Source="Resources\ModelTemplates.xaml" />
                <ResourceDictionary Source="Resources\ViewModelTemplates.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
